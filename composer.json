{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 9 projects with Composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "php": ">=7.4",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/admin_toolbar": "^3.0",
        "drupal/adminimal_theme": "^1.6",
        "drupal/advagg": "^4.1",
        "drupal/advpoll": "^1.2",
        "drupal/backup_migrate": "^5.0",
        "drupal/better_exposed_filters": "^5.0",
        "drupal/bootstrap5": "^1.0",
        "drupal/chart_suite": "^1.1",
        "drupal/charts": "^5.0",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-project-message": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/ctools": "^3.7",
        "drupal/devel": "^4.1",
        "drupal/entity": "^1.3",
        "drupal/entity_print": "^2.6",
        "drupal/facets": "^1.8",
        "drupal/field_group": "^3.2",
        "drupal/google_analytics": "^2.5",
        "drupal/metatag": "^1.16",
        "drupal/mobile_detect": "^3.0",
        "drupal/paragraphs": "^1.14",
        "drupal/pathauto": "^1.8",
        "drupal/poll": "^1.5",
        "drupal/range": "^1.3",
        "drupal/redirect": "^1.6",
        "drupal/stage_file_proxy": "^1.2",
        "drupal/theme_switcher": "^1.2",
        "drupal/token": "^1.10",
        "drupal/twig_tweak": "^3.1",
        "drupal/typed_data": "^1.0@beta",
        "drupal/user_csv_import": "^2.0",
        "drupal/views_bulk_operations": "^4.1",
        "drupal/views_data_export": "^1.1",
        "drupal/webform": "^6.1",
        "drupal/webform_analysis": "^1.0@beta",
        "drupal/webform_calculation": "^2.0",
        "drupal/webform_score": "^2.1",
        "drupal/webform_views": "^5.0@beta",
        "drupal/wkhtmltopdf": "^1.3",
        "drush/drush": "^10.6",
        "mikehaertl/phpwkhtmltopdf": "~2.1",
        "tecnickcom/tcpdf": "~6",
        "vlucas/phpdotenv": "^5.1",
        "webflo/drupal-finder": "^1.2"
    },
    "require-dev": {
        "drupal/core-dev": "^9.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "discard-changes": true,
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
        }
    }
}
